<h1>list of contacts</h1>

<form action="/contact" method="get">
  <input type="text" name="member" placeholder="Who are you looking for?" value="<%= params[:member] %>">
  <input type="submit">
</form>

<ul>
  <% @members.each do |member| %>
    <li> <%= member.capitalize %> </li>
  <% end %>
</ul>

